allow platform_app sysfs_graphics:file r_file_perms;
allow platform_app vendor_fp_prop:file {  getattr map read open };
allow platform_app cgroup:file r_file_perms;
allow platform_app vendor_default_prop:file { getattr open read map };
dontaudit platform_app boottime_prop:file r_file_perms;
dontaudit platform_app time_daemon:unix_stream_socket connectto;
dontaudit platform_app time_daemon:file { read };
dontaudit platform_app vendor_audio_prop:file { r_file_perms };
